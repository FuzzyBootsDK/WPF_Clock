<Window x:Class="WPF_CLock.ApiKeyDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="API Key" Height="150" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Enter your API Key:" Margin="0,0,0,10"/>
            <TextBox Name="ApiKeyTextBox" Width="360"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,10,0,0">
                <Button Content="OK" Width="75" Margin="5" Click="OkButton_Click"/>
                <Button Content="Cancel" Width="75" Margin="5" Click="CancelButton_Click"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
